{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://changemehostname.ankr.com:80"
      },
      "HttpsInlineCertAndKeyFile": {
        "Url": "https://changemehostname.ankr.com:443",
        "Certificate": {
          "Path": "/etc/nginx/ankr/ssl/_.ankr.com.crt",
          "KeyPath": "/etc/nginx/ankr/ssl/_.ankr.com.key"
        }
      }
    }
  }
}