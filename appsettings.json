{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://changemehostname.ankr.com:80"
      },
      "HttpsInlineCertAndKeyFile": {
        "Url": "https://changemehostname.ankr.com:443",
        "Certificate": {
          "Path": "/etc/nginx/ankr/ssl/_.ankr.com.crt",
          "KeyPath": "/etc/nginx/ankr/ssl/_.ankr.com.key"
        }
      }
    }
  },
  "NodeConfigs": [
    {
      "ClusterId": "chain_client_node_number",
      "NodePath": "/chain_client_node_number",
      "NodePathExtension": "/optionalpathextension",
      "NodeDetail": {
        "RPCAddress": "http://127.0.0.1:12341",
        "WSAddress": "http://127.0.0.1:12342"
      }
    }
  ]
}